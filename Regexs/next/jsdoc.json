{
  "opts": {
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["better-docs/typescript", "better-docs/category", "plugins/markdown"],
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(node_modules/|docs)"
  }
}
