{
  "name": "payroll",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "run-p -cs lint:*",
    "lint:eslint": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "lint:stylelint": "stylelint ./src/styles/*.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "doc": "jsdoc -c jsdoc.json",
    "json-server": "json-server --watch db.json --port 3004",
    "check-dependency-circular": "madge --circular src",
    "make-dependency-graph": "madge --dot --image dependency/dependencygraph.svg src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "load-test": "artillery run load-test.yml",
    "load-test:report": "artillery run -o load-test-report.json load-test.yml && artillery report load-test-report.json",
    "apidoc": "apidoc -i src/pages/api -i src/util/api -o apidoc"
  },
  "repository": {
    "type": "git",
    "url": "http://noel.yayoi-kk.co.jp/nextgen/nextgen.git"
  },
  "author": "Yayoi Co., Ltd.",
